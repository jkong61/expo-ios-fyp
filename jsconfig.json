{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets": ["assets/"],
      "@components": ["components/"],
      "@components/*": ["components/*"],
      "@config/*": ["config/*"],
      "@utilities/*": ["utilities/*"],
      "@http/*": ["http/*"],
      "@screens/*": ["screens/*"],
      "@navigation/*" : ["./navigation/*"],
      "@async_storage/*" : ["./async_storage/*"]
    },
  },
  "exclude": ["node_modules"]
}